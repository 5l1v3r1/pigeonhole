require "vnd.dovecot.testsuite";
require "enotify";

test "Execute" {
	/* Test to catch runtime segfaults */
	if valid_notify_method 
		"mailto:alm@example.com" {
		stop;
	}

	/* Test to catch runtime segfaults */
	notify 
		:message "This is probably very important"
		:importance "1" 
		"mailto:alm@example.com";
}
